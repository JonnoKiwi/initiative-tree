    "reactotron-redux-saga": "^4.2.3",
    "redux": "^4.0.5",
    "redux-reset": "^0.3.0",
    "redux-saga": "^1.1.3",
    "reduxsauce": "^1.2.0",
    "seamless-immutable": "^7.1.4",
    "validate.js": "0.13.1"
